{
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "printWidth": 80,
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "printWidth": 80,
        "proseWrap": "always",
        "tabWidth": 4
      }
    }
  ]
}
